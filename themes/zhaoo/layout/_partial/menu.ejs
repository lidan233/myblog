<nav class="menu">
  <div class="menu-wrap">
    <div class="menu-close">
      <i class="iconfont iconbaseline-close-px"></i>
    </div>
    <ul class="menu-content">
      <% for (var i in theme.menu) { %>
      <% var arr = theme.menu[i].split('||') %>
      <% var url = arr[0] %>
      <% var name = arr[1] %>
      <li class="menu-item"><a href="<%- url %>" class="underline"><%- name %></a></li>
      <% } %>
    </ul>
    <div class="menu-copyright"><%- theme.copyright %></div>
  </div>
</nav>

